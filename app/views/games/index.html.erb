<h1>Here is my game collection</h1>
<table>

  <% @games.each do |game| %>
    <tr>
      <td><%= game.name %></td>
      <td><%= link_to "GameInfo", game_path(game) %></td>
      <td><%= link_to "Edit", edit_game_path(game) %></td>
      <td><%= link_to "Delete", game_path(game), :method => :delete %></td>
    </tr>


  <% end %>
</table>

<%= link_to "Add New Game", new_game_path %>

<%= paginate @games %>


